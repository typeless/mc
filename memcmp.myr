use std

const memcmp = {a : byte[:], n
	var i = 0
	//var b : byte[:] = "abcef"
 	while i < n 
		if a[i] != "abcdef"[i]
			->false
		;;
		i++
	;;
	-> true
}

const main = {
	var a = "abc"
	var b = "abd"
	var n

	if a.len <= b.len
		n = a.len
	else
		n = b.len
	;;

	if memcmp(a, n)
		std.put("matched")
	;;
}
